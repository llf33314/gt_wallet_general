    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, w_member_id AS wMemberId, member_num AS memberNum, cardNo, phone, card_check AS cardCheck, 
        identityType, identityNo, validate, cvv2, is_safe_card AS isSafeCard, union_bank AS unionBank, tranceNum,
